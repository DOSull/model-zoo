globals [
  N       ;; total number of sites (i.e. patches)
  pcolors ;; a list of the colours for rendering each state
]

patches-own [
  ;; store particle type as an integer
  particle-type
]

to setup
  clear-all

  set pcolors (list (grey + 2) red black)
  set N count patches
  ask patches [
    ifelse random-float 1 < initial-proportion
    ;; one of the live types, with equal probability
    [ set particle-type random 2 + 1 ]
    ;; the dead 0 type
    [ set particle-type random 0 ]
  ]
  color-patches
  reset-ticks
end

to go
  ;; run a full round or 'generation' of update events
  repeat N [
    ask one-of patches [
      if particle-type > 0 [ ;; if particle is present
        ifelse random 2 = 1 ;; with equal probability it dies or gives birth
        [ death-event ]
        [ birth-event ]
      ]
    ]
  ]
  color-patches
  tick
end

;; death is the same for each particle type,
;; but occurs with different probabilities
to death-event
  ifelse particle-type = 1 [
    if random-float 1 < p-death-1 [
      set particle-type 0
    ]
  ]
  [
    if random-float 1 < p-death-2 [
      set particle-type 0
    ]
  ]
end


;; birth is the same for each state, being a local
;; propagation to a randomly selected adjacent site, but state 2 may
;; optionally propagate to a non-neighboring sites
to birth-event
  ifelse particle-type = 1 [
    ask one-of neighbors4 [
      if particle-type = 0 [
        set particle-type 1
      ]
    ]
  ]
  [ ;; for type 2 since long-range propagation is possible
    ;; we first select the target depending on the parameter setting
    let target self
    ifelse long-range-2?
    [ ;; target site may be anywhere in the system
      ;; except 'self'. Could use 'one-of other patches' but
      ;; it is relatively slow. This 'with' clause is quicker
      set target one-of (patches with [self != myself])
      ;; this code is slower:
      ;; set target one-of other patches
    ]
    [ ;; else it is a neighbouring site
      set target one-of neighbors4
    ]
    ask target [
      if particle-type = 0 [
        set particle-type 2
      ]
    ]
  ]
end


;; color patches using particle-type as index into the list of colors
to color-patches
  ask patches [
    set pcolor item particle-type pcolors
  ]
end
