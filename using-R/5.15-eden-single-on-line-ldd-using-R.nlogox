<?xml version="1.0" encoding="utf-8"?>
<model version="NetLogo 7.0.0" snapToGrid="false">
  <code><![CDATA[extensions [sr palette]
__includes["5.x-r-clusters.nls"]

globals [
  perimeter
  p-length-list
  n-occupied
  max-height
  mean-height-list
  var-height-list
]

breed [counters counter]
counters-own [hgt]

patches-own [
  t-colonised
  attempts
  occupied?
]

to setup
  clear-all
  sr:setup
  sr:run "library(matlab)"

  ask patches with [pycor = 0] [
    sprout-counters 1 [set hgt 0 ht]
  ]

  set n-occupied 0
  ask patches [
    set pcolor sky - 1
    set occupied? false
    set t-colonised -1
  ]
  let start-sites patches with [pycor = 0]
  set perimeter nobody
  ask start-sites [
    let f-site self
    set occupied? true
    set t-colonised 0
    set perimeter (patch-set perimeter neighbors4)
    set pcolor white
    set n-occupied n-occupied + 1
    ask counters with [xcor = ([pxcor] of f-site)] [set hgt hgt + 1]   ;; f-site = myself
  ]
  set max-height 1
  set mean-height-list []
  set var-height-list []
  set mean-height-list lput 1 mean-height-list
  set var-height-list lput 0 var-height-list
  set p-length-list []
  set p-length-list lput (count perimeter) p-length-list
  reset-ticks
end

to go
  ;; initiate the growth
  if max-height > max-pycor - 2 [stop]

  if ticks mod 50 = 0
  [
   set mean-height-list lput get-mean-height mean-height-list
   set var-height-list lput get-variance-height var-height-list
  ]

  ask one-of perimeter
  [
    set attempts attempts + 1

    if attempts >= m
    [
      let new-site self
      set occupied? true
      set n-occupied n-occupied + 1
      set pcolor [103 169 207]
      set t-colonised ticks

      let new-edge-cells neighbors4 with [occupied? = false]

      ;; rebuilds the perimeter-list with the new candidates added and the newly colonised patch removed
      set perimeter (patch-set (other perimeter) new-edge-cells)

      ;; long-distance dispersal?
      if random-float 1 < long-jump-freq
      [
        let jump-x [pxcor] of new-site
        let jump-y 0

        if ldd-distribution = "exponential"
         [ set jump-y [pycor] of new-site  + random-exponential long-jump-scale ]
        if ldd-distribution = "cauchy"
         [
           sr:set "ljs" long-jump-scale
           set jump-y [pycor] of new-site  + sr:runresult "abs(rcauchy(1,0,ljs))" ]
        if ldd-distribution = "normal"
         [ set jump-y [pycor] of new-site  + abs (random-normal 0 long-jump-scale) ]

        ;; IS THERE A PROBLEM HERE?  ADDING the jump a second time?
        ;; ALSO... when LDD occurs there are 2 colonisation events.  Is
        ;; this a problem?
        set jump-y round jump-y ; ([pycor] of new-site + jump-y)

        ;; stop jumps beyond the edge of the lattive
        if jump-y >= max-pycor - 2
           [set jump-y max-pycor - 2]

        let jump-site patch jump-x jump-y

        ask jump-site
        [
          set occupied? true
          set n-occupied n-occupied + 1
          set pcolor [103 169 207]
          set t-colonised ticks
          set new-edge-cells neighbors4 with [occupied? = false]
          set perimeter (patch-set (other perimeter) new-edge-cells)
        ]
      ]
      ;; update the counters etc. -> rather arcane as need to make sure we have the uppermost
      ;; patch irrespective of holes etc.
      let f-counter counters with [xcor = ([pxcor] of new-site)]
      let h max [hgt] of f-counter     ;; list to number

      if h < ([pycor] of new-site) + 1
        [ ask f-counter [ set hgt ([pycor] of new-site) + 1 ] ]

      if pycor + 1 > max-height [set max-height pycor + 1]
    ]

  ]
  if ticks mod 500 = 0 [ colour-by-time false ]
  set p-length-list lput (count perimeter) p-length-list
  
  tick
end

;; colour patches by the time they were colonised (dark [old] to light [young])
to colour-by-time [reset?]
  if reset? [ ask patches [ set pcolor sky - 1] ]
  ask patches with [occupied?] [
    set pcolor palette:scale-gradient [[239 138 98] [247 247 247] [103 169 207] ]  t-colonised 0 ticks
  ]
end

;; highlight the perimeter cells only
to colour-edge
  ;;ask patches [set pcolor black]
  ask perimeter [set pcolor red]
end

;; send various graphics to R
to variance-to-r
  sr:set "v" var-height-list
  sr:run "t <- seq(1, length(v))"
  sr:set-plot-device
  sr:run "plot(v ~ t, type = 'l', las = 1, xlab = 'Time', ylab = 'Variance height')"
end

to mean-to-r
  sr:set "m" mean-height-list
  sr:set-plot-device
  sr:run "t <- seq(1, length(m))"
  sr:run "plot(m ~ t, type = 'l', las = 1, xlab = 'Time', ylab = 'Mean height')"
end

to-report get-mean-height
  report mean [hgt] of counters
end

to-report get-variance-height
  report variance [hgt] of counters
end

